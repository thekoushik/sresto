{
    "name": "thekoushik/sresto",
    "description": "A very simple lightweight PHP REST Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Koushik Seal",
            "email": "thekoushik.universe@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "files": [
            "src/Common/commons.php"
        ],
        "psr-4": {
            "SRESTO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SRESTO\\Tests\\": "tests/"
        }
    }
}
