{
    "name": "thekoushik/sresto-starter",
    "description": "Starter project for SRESTO Framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "thekoushik",
            "email": "thekoushik.universe@gmail.com"
        }
    ],
    "repositories": [
        {
            "type":"git",
            "url":"https://github.com/thekoushik/sresto.git"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "thekoushik/sresto": "dev-development",
        "symfony/dependency-injection": "3.3",
        "symfony/validator": "3.3",
        "symfony/yaml": "3.3",
        "doctrine/orm": "2.5.*"
    },
    "autoload": {
        "psr-4": {
            "API\\":"src/API/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
